<mnx xmlns="https://github.com/notator/mnx"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://github.com/notator/mnx ../schema/common1900.xsd">
    <global>
        <measure-global>
            <directions-global>
                <time signature="4/4"/>
            </directions-global>
        </measure-global>
    </global>
    <part name="Music">
        <measure>
            <directions-part>
                <clef line="2" sign="G"/>
            </directions-part>
            <sequence>
                <beams>
                    <beam events="ev1 ev3 ev4 ev5"/>
                </beams>
                <event id="ev1" value="/8">
                    <note pitch="C5"/>
                </event>
                <grace>
                    <event value="/8">
                        <note pitch="B4"/>
                    </event>
                </grace>
                <event id="ev3" value="/8">
                    <note pitch="D5"/>
                </event>
                <event id="ev4" value="/8">
                    <note pitch="E5"/>
                </event>
                <event id="ev5" value="/8">
                    <note pitch="F5"/>
                </event>
                <event value="/2">
                    <rest/>
                </event>
            </sequence>
        </measure>
    </part>
</mnx>